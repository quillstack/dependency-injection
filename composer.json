{
    "name": "quillstack/di",
    "description": "The dependency injection container based on PSR-11: Container interface.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dependency-injection",
        "container",
        "di",
        "di-container",
        "psr-11",
        "php8",
        "class",
        "properties",
        "property",
        "constructor"
    ],
    "homepage": "https://quillstack.org/di",
    "authors": [
        {
            "name": "Radek Ziemniewicz",
            "email": "radek@quillstack.org"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/container": "^1.0"
    },
    "autoload": {
        "files": [
            "src/container.helper.php"
        ],
        "psr-4": {
            "Quillstack\\DI\\": "src/",
            "Quillstack\\DI\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "quillstack/unit-tests": "^0.2.4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    }
}
